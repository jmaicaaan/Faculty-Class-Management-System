<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">

  <struts>
  	<constant name="struts.devMode" value="true"/>
  	
  	
  	<package name="projects" extends="json-default">
  		<interceptors>
	   		<interceptor-stack name="defaultStack">
	   			<interceptor-ref name="json">
	   				<param name="enableSMD">true</param>
	   			</interceptor-ref>
	   		</interceptor-stack>
	   	</interceptors>
  	
  		<action name="uploadProject" class="com.action.profiling.ProjectAction">
	  		<result type="json">
	  			<param name="">pModel</param>
	  		</result>
  		</action>
  		
  		<action name="uploadResearch" class="com.action.profiling.ResearchAction">
	  		<result name="success" type="json">
	  			<param name="">rModel</param>
	  		</result>
  		</action>
  		
  		<action name="saveProfessors" class="com.action.developer.DeveloperAction">
   			<interceptor-ref name="json">
	   			<param name="enableSMD">true</param>
	   		</interceptor-ref>
	   		<result name="success" type="json"></result>
   		</action>

  	</package>
  	<package name="default" extends="struts-default, json-default">
  	<package name="achievements" extends="struts-default, json-default">
  	<package name="default" extends="struts-default, json-default">
  		<action name="upload_Achievement_Certificate" class="com.action.profiling.Achievement_Certificate_Action">
  			<result type="json">
  				<param name="">acModel</param>
  			</result>
  		</action>
  		<action name="uploadResume" class="com.action.profiling.ResumeAction">
  			<result type="json">
  				<param name="">rModel</param>
  			</result>
  		</action>
  	</package>
  	<!-- <include file="developer.xml"></include> -->
  	
  	
  </struts>
