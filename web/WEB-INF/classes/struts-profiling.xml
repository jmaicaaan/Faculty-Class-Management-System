<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">

   <struts>
   	<constant name="struts.devMode" value="true"/>
	   <package name="profiling" extends="struts-default, json-default">
	  	   		 
		   <interceptors>
		   		<interceptor-stack name="myStack">
		   			<interceptor-ref name="defaultStack"/>
		   			<interceptor-ref name="fileUpload"/>
		   			<interceptor-ref name="json">
		   				<param name="enableSMD">true</param>
		   			</interceptor-ref>
		   		</interceptor-stack>
		   	</interceptors>
		   	
		   	<action name="upload_Achievement_Certificate" 
	  				class="com.action.profiling.Achievement_Certificate_Action">
	  				<interceptor-ref name="myStack"/>
	  			<result name="success" type="json">
	  				<param name="">acModel</param>
	  			</result>
	  			<result name="input" type="json">
		  			<param name="statusCode">500</param>
		  			<param name="errorCode">500</param>
		  		</result>
	  			
	  		</action>
	  	
	  		<action name="uploadProject" class="com.action.profiling.ProjectAction">
	  			<interceptor-ref name="myStack"/>
		  		<result name="success" type="json">
		  			<param name="">pModel</param>
		  		</result>
		  		<result name="input" type="json">
		  			<param name="statusCode">500</param>
		  			<param name="errorCode">500</param>
		  		</result>
	  		</action>
	  		
	  		<action name="uploadResearch" class="com.action.profiling.ResearchAction">
	  			<interceptor-ref name="myStack"/>
		  		<result name="success" type="json">
		  			<param name="">rModel</param>
		  		</result>
		  		<result name="input" type="json">
		  			<param name="statusCode">500</param>
		  			<param name="errorCode">500</param>
		  		</result>
	  		</action>
	  	
	  		<action name="uploadResume" class="com.action.profiling.ResumeAction">
	  			<result name="success" type="json">
	  				<param name="">rModel</param>
	  			</result>
	  		</action>
	  		
	  		<action name="uploadUserImage" class="com.action.profiling.UploadUserImageAction">	  			
	  			<interceptor-ref name="myStack"/>
	  			<result name="success" type="json">
	  			
	  			</result>
	  			<result name="input" type="json">
		  			<param name="statusCode">500</param>
		  			<param name="errorCode">500</param>
		  		</result>
	  		</action>
	  		
	  		<action name="updateUserProfile" class="com.action.profiling.UpdateUserProfileAction">
	  			<interceptor-ref name="myStack"/>
	  			<result name="success" type="json">
	  				<param name="excludeNullProperties">true</param>
	  			</result>
	  			<result name="input" type="json">
		  			<param name="statusCode">500</param>
		  			<param name="errorCode">500</param>
		  		</result>
	  		</action>
	  		
	  		<action name="updateUserPassword" class="com.action.profiling.UpdateUserPasswordAction">
	  			<interceptor-ref name="myStack"/>
	  			<result name="success" type="json">
	  				<param name="excludeNullProperties">true</param>
	  			</result>
	  			<result name="input" type="json">
		  			<param name="statusCode">500</param>
		  			<param name="errorCode">500</param>
		  		</result>
	  		</action>
	  		
	  		<action name="updateCurrentSession" class="com.action.profiling.UpdateCurrentSessionAction">
	  			<interceptor-ref name="myStack"/>
	  			<result name="success" type="json">
	  				<param name="excludeNullProperties">true</param>
	  			</result>
	  			<result name="input" type="json">
		  			<param name="statusCode">500</param>
		  			<param name="errorCode">500</param>
		  		</result>
	  		</action>
	  		
	
	  	</package>
   </struts>